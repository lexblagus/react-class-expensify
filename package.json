{
  "name": "expensify",
  "description": "React classes",
  "repository": "",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Lex Blagus",
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json --forceExit --verbose",
    "test:updateSnapshot": "cross-env NODE_ENV=test jest --config=jest.config.json --forceExit --verbose --updateSnapshot",
    "test-watch": "npm test -- --watchAll",
    "dev-server": "webpack-dev-server",
    "dev": "nodemon -e js,json,jsx,test.js,sh,html,css,scss,babelrc,eslintrc.js --exec \"./scripts/dev.sh\"",
    "build:dev": "webpack",
    "build:prod": "webpack -p --env production",
    "start": "node server/server.js",
    "heroku-postbuild": "npm run build:prod"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "scripts",
      "node_modules",
      ".git"
    ],
    "REM__watch": ["./"],
    "ext": "js json jsx html css scss"
  },
  "dependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "blueprint-css": "^2.4.2",
    "bulma": "^0.6.1",
    "console": "^0.5.2",
    "css-loader": "0.28.4",
    "ejs": "^2.5.7",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.0",
    "firebase": "^4.8.0",
    "live-server": "^1.2.0",
    "milligram": "^1.3.0",
    "moment": "^2.19.3",
    "node-sass": "4.5.3",
    "nodemon": "^1.12.5",
    "normalize.css": "^7.0.0",
    "raf": "^3.4.0",
    "react": "15.6.1",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dates": "^12.7.1",
    "react-dom": "15.6.1",
    "react-modal": "2.2.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-to-html-webpack-plugin": "^2.2.0",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "uuid": "^3.1.0",
    "validator": "8.0.0",
    "webpack": "3.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.2.0",
    "webpack-dev-server": "2.5.1"
  }
}
